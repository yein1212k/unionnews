                        </ul>
                    </div>
                </div>
			</div>
			<!-- //local-head -->
			<!-- local-body -->
			<div class="local-body" id="content">
				<!-- content-body -->
				<div class="content-body">
                    <div class="no-result">
                        <div class="no-result-inner">
                            <p class="des">검색결과가 없습니다.</p>
                        </div>
                    </div>
				</div>
				<!-- //content-body -->
			</div>
			<!-- //local-body -->
		</div>
		<!-- //page-body -->
      <%-include('../_templates/_guide.footer.ejs') %>
